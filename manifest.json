{
    "manifest_version": 2,
    "name": "I Love Cookie",
    "version": "1.0",
    "description": "Cookies is good!",
    "permissions":[
        "contextMenus",
        "storage"
    ],
    "background":{
        "scripts":["eventPage.js"],
